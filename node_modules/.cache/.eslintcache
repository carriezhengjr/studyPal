local/studyPal/src/components/Timer.js",["124"],[],"import React, { Component } from \"react\";\nimport Countdown from \"./Countdown\";\n\nfunction Timer () {\n    return (\n        <div class=\"timer\">\n            <h1 class=\"title\">Timer</h1>\n            <div className=\"timer-countdown\">\n                <Countdown />\n            </div>\n        </div>\n    )\n}\n\nexport default Timer;","/Users/ongyongen/Desktop/studyPal-local/studyPal/src/components/Calendar.js",[],[],"/Users/ongyongen/Desktop/studyPal-local/studyPal/src/components/Todolist.js",["125","126","127","128","129"],[],"import React from \"react\";\nimport Card from './Card.js';\nimport UserTasks from './UserTasks.js';\nimport AddTodo from './AddTodo.js'\nimport Todo from './Todo.js'\n\n\nimport {db} from \"../firebase.js\"\n\nimport {\n    collection,\n    query,\n    onSnapshot,\n    doc,\n    updateDoc,\n    deleteDoc,\n    QuerySnapshot\n  } from \"firebase/firestore\"\n  \nimport { useState, useEffect } from \"react\"\n\nexport default function Todolist () {\n    const [todos, setTodos] = React.useState([]);\n\n  React.useEffect(() => {\n    const q = query(collection(db, \"todos\"));\n    const unsub = onSnapshot(q, (querySnapshot) => {\n      let todosArray = [];\n      querySnapshot.forEach((doc) => {\n        todosArray.push({ ...doc.data(), id: doc.id });\n      });\n      setTodos(todosArray);\n    });\n    return () => unsub();\n  }, []);\n\n  const handleEdit = async (todo, title) => {\n    await updateDoc(doc(db, \"todos\", todo.id), { title: title });\n  };\n  const toggleComplete = async (todo) => {\n    await updateDoc(doc(db, \"todos\", todo.id), { completed: !todo.completed });\n  };\n  const handleDelete = async (id) => {\n    await deleteDoc(doc(db, \"todos\", id));\n  };\n  return (\n    <div className=\"App\">\n      <div>\n        <AddTodo />\n      </div>\n      <div className=\"todo_container\">\n        {todos.map((todo) => (\n          <Todo\n            key={todo.id}\n            todo={todo}\n            toggleComplete={toggleComplete}\n            handleDelete={handleDelete}\n            handleEdit={handleEdit}\n          />\n        ))}\n      </div>\n    </div>\n  );\n}\n\n\n/*\n // To-do list Cards\n    const userTasks = UserTasks.map(item => {\n        return <Card\n                    task = {item.task}\n                    day = {item.day}\n                    urgency = {item.urgency}\n                />\n    });\n\n    return (\n        <>\n        <div class=\"container\">\n\n            <h1 class=\"title\">To-do List</h1>\n            <form>\n                <input className=\"todoinput\"></input>\n                <button className=\"todobutton\">Add Task</button>\n            </form>\n        </div>\n\n        <div className=\"cards\">\n            {userTasks}\n        </div>\n        \n        </>\n    );\n}\n*/","/Users/ongyongen/Desktop/studyPal-local/studyPal/src/components/Home.jsx",[],[],"/Users/ongyongen/Desktop/studyPal-local/studyPal/src/components/Achievements.js",[],[],"/Users/ongyongen/Desktop/studyPal-local/studyPal/src/components/Login.jsx",["130"],[],"import React, { useState } from 'react';\nimport { Link, useNavigate } from 'react-router-dom';\nimport { UserAuth } from '../context/AuthContext';\n\nconst Signin = () => {\n  const [email, setEmail] = useState('');\n  const [password, setPassword] = useState('');\n  const [error, setError] = useState('');\n  const navigate = useNavigate();\n  const { signIn } = UserAuth();\n\n  const handleSubmit = async (e) => {\n    e.preventDefault();\n    setError('')\n    try {\n      await signIn(email, password)\n      navigate('/home')\n    } catch (e) {\n      setError(e.message)\n      console.log(e.message)\n    }\n  };\n\n  return (\n    <div className='login'>\n      <div>\n        <h1>Log in to your account</h1>\n        <p>\n          Don't have an account yet?{' '}\n          <Link to='/signup' className='underline'>Sign up.</Link>\n        </p>\n      </div>\n      <div>\n        <form class='login' onSubmit={handleSubmit}>\n          <div>\n            <label class='login-label'>Email Address</label>\n            <input class='login-input' \n                   onChange={(e) => setEmail(e.target.value)} \n                   type='email' \n            />\n          </div>\n          <div>\n            <label class='login-label'>Password</label>\n            <input class='login-input' onChange={(e) => setPassword(e.target.value)} type='password' />\n          </div>\n          <div>\n            <button class='login-button'>Log In</button>\n            <p><Link to='/'>Home</Link></p>\n          </div>\n        </form>\n      </div>\n    </div>\n  );\n};\n\nexport default Signin;\n/*\nimport userEvent from '@testing-library/user-event'\nimport { Link, useNavigate } from 'react-router-dom'\nimport { UserAuth } from '../context/AuthContext'\nimport { useState } from 'react'\nimport { signInWithRedirect } from 'firebase/auth'\n\nconst Login = () => {\n    const [email, setEmail] = useState('')\n    const [password, setPassword] = useState('')\n    const [error, setError] = useState('')\n    const navigate = useNavigate()\n    const { logIn } = UserAuth()\n\n    const handleSubmit = async (e) => {\n        e.preventDefault();\n        setError('')\n        try {\n            await logIn(email, password)\n            navigate('/Home')\n        } catch (e) {\n            setError(e.message)\n            console.log(e.message)\n        }\n    }\n \n\n    return (\n        <div>\n            <h1>Login</h1>\n            <p>Do not have an account?<Link to='/signup'>Sign up</Link></p>\n            <form >\n                <div>\n                    <label>Email Address</label>\n                    <input \n                        onChange={(e) => setEmail(e.target.value)}\n                    />\n                </div>\n                <div>\n                    <label>Password</label>\n                    <input \n                        onChange={(e) => setPassword(e.target.value)}\n                    />\n                </div>\n                <div>\n                    <button>Submit</button>\n                    <p><Link to='/'>Home</Link></p>\n                </div>\n            </form>\n        </div>\n    )\n}\n\nexport default Login;\n\n/*\nimport { Link, useNavigate } from 'react-router-dom'\nimport { UserAuth } from '../context/AuthContext'\n\nconst Login = () => {\n    const { logIn } = UserAuth();\n\n    return (\n        <div>\n            <h1>Login</h1>\n            <p>Do not have an account?<Link to='/signup'>Sign up</Link></p>\n            <form>\n                <div>\n                    <label>Email Address</label>\n                    <input type='email'></input>\n                </div>\n                <div>\n                    <label>Password</label>\n                    <input type='password'></input>\n                </div>\n                <div>\n                    <button>Submit</button>\n                    <p><Link to='/'>Home</Link></p>\n                </div>\n          </form>\n        </div>\n    )\n}\n\nexport default Login;\n*/\n","/Users/ongyongen/Desktop/studyPal-local/studyPal/src/components/About.js",[],[],"/Users/ongyongen/Desktop/studyPal-local/studyPal/src/components/LandingPage.jsx",[],[],"/Users/ongyongen/Desktop/studyPal-local/studyPal/src/components/Signup.jsx",["131"],[],"import { useState } from 'react'\nimport { Link, useNavigate } from 'react-router-dom'\nimport { UserAuth } from '../context/AuthContext'\n\nconst Signup = () => {\n    const [email, setEmail] = useState('')\n    const [password, setPassword] = useState('')\n    const [error, setError] = useState('')\n\n    const { createUser } = UserAuth() \n    const navigate = useNavigate()\n\n    const handleSubmit = async (e) => {\n        e.preventDefault()\n        setError('')\n        try {\n            await createUser(email, password)\n            navigate('/Home')\n        } c